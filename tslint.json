{
    "extends": "tslint:recommended",
    "rules": {
        "quotemark": ["single", "avoid-escape"],
        "trailing-comma": [],
        "no-var-keyword": false,
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": [],
        "jsdoc-format": false,
        "prefer-for-of": false,
        "no-empty": false,
        "no-string-literal": false,
        "max-line-length": []
    },
    "jsRules": {
        "quotemark": ["single", "avoid-escape"],
        "trailing-comma": []
    }
}
